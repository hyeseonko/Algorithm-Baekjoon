#include <stdio.h>int dp[1001];int main(){    int N; scanf("%d", &N);    if(N<100) printf("%d\n", N);    else    {        for(int i=1;i<100;i++) dp[i]=i;        if(N==1000) N=999;        for(int i=100;i<=N;i++)        {        int first=i/100;        int second=(i%100)/10;        int third=i%10;        if(first-second==second-third) dp[i]=dp[i-1]+1;        else dp[i]=dp[i-1];        }        printf("%d\n", dp[N]);    }    return 0;}
